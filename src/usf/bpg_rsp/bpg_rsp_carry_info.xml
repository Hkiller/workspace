<?xml version="1.0" encoding="GBK" standalone="yes" ?>
<metalib tagsetversion="1" name="BASEPKG" version="1" >
    <struct name="bpg_carry_info" version="1">
        <entry name="clientId" type="uint64"/>
        <entry name="sn" type="uint32"/>
        <entry name="cmd" type="uint16"/>
        <entry name="no_response" type="uint16"/>
    </struct>

    <struct name="bpg_carry_meta" version="1">
        <entry name="name" type="string" size="64"/>
    </struct>

    <struct name="bpg_carry_metas" version="1">
        <entry name="count" type="uint32"/>
        <entry name="data" type="bpg_carry_meta" count="0" refer="count"/>
    </struct>

</metalib>
